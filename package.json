{
  "name": "viztein",
  "version": "0.1.7",
  "description": "Viztein React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "ngl": "^0.10.4"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.14",
    "lint-staged": "^7.2.2",
    "nwb": "0.23.x",
    "prettier": "1.14.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0"
  },
  "author": "Josh McMenemy <josh.mcmenemy@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component",
    "protein-vizualization",
    "molecular-graphics",
    "scientific-visualization",
    "webgl",
    "ngl",
    "react"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
